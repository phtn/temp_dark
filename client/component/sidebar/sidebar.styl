.index-lbl
	color #444
	text-transform uppercase
	letter-spacing 2px
	font-family 'Roboto', sans-serif !important
	font-weight bold
	font-size 10px
	margin-left 15px
	//text-align center
	

.sidebar-ul
	
	font-family 'Roboto', sans-serif
	font-weight bolder
	border-radius 3px !important
	margin-top 10px
	li
		a
			color #79ABFF
		a:focus
			background-color #161718
			outline none !important
	li:hover
		a
			background-color #333
			color #D3D5D4
			border none !important
			border-radius 3px !important
			
	
		

.active
	a
		background-color #1ca8dd !important
		color #161718 !important
		font-family 'Roboto', sans-serif
		font-weight bolder
		border-radius 2px !important
		//text-transform uppercase
		
.folder-icon
	margin-right 15px
	
#home
	animation-duration .5s
	animation-delay .1s
#notes
	animation-duration .5s
	animation-delay .2s
#todo
	animation-duration .5s
	animation-delay .3s
#about
	animation-duration .5s
